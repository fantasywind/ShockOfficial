nav#publish-navigation
  a(ng-repeat='nav in navs', ng-click='moveTo(nav.src)', ng-class='nav.name === page ? "active" : ""')
    span(translate='{{nav.name}}')
    ul.sub(ng-show='nav.children.length')
      li(ng-repeat='sub in nav.children', ng-click='moveTo(sub.src)')
        span(ng-href='sub.src', translate='{{sub.name}}')